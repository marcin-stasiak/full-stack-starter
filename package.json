{
  "name": "starter",
  "version": "1.0.0",
  "description": "Starter",
  "author": "Marcin Stasiak",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist .next",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "build:client": "NODE_ENV=production cd src/client && next build",
    "build:server": "NODE_ENV=production nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node app.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@casl/ability": "^5.4.3",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-express": "^3.6.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "graphql": "^15.8.0",
    "helmet": "^5.0.1",
    "next": "^12.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.41"
